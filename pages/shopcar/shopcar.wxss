/* shopcar.wxss */
page{
	width:100%;
	height:100%;
}
@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}
.title:before{
	font-family:'iconfont';
	content:'\e6c5';
	font-size:'28rpx';
}
.chopcar-container{
	width:100%;
	height:100%;
	border-top:1rpx solid #ccc;
	position:relative;
	box-sizing:border-box;
}
.choose{
	margin:0 20rpx;
}
.cartitle{
	height:100rpx;
	width:100%;
	display:flex;
	align-items:center;
	box-sizing:border-box;
	position:relative;
}
.edit{
	position:absolute;
	right:30rpx;
	font-size:28rpx;
	color:#ccc;
}
.title{
	font-size:28rpx;
}
.cartotal{
	position:absolute;
	left:0;
	bottom:0;
}

/*o购物车列表*/
.item-box{
	height:230rpx;
	border-bottom:1px solid #ccc;
}
.item{
	display:flex;
	padding:0 30rpx;
	align-items:center;
	background:rgb(247,249,250);
	padding:10rpx 0;
	position:relative;
}
.item .center{
	height:175rpx;
	display:flex;
	flex-direction:column;
	justify-content:space-between;
}
.goodsname{
	font-size:28rpx;
}
.buycount{
	color:red;
}
.num{
	position:absolute;
	right:30rpx;
	bottom:30rpx;
}
/*阶段区域*/
.cartotal{
	height:105rpx;
	width:100%;
	border-top:1px solid #ccc;
	display:flex;
}
.account{
	width:30%;
	background:red;
	text-align:center;
	line-height:105rpx;
	color:#fff;
}
.totalleft{
	width:70%;
	display:flex;
	align-items:center;
}
.totall-box{
	flex:1;
	height:105rpx;
	display:flex;
	flex-direction:column;
	align-items:flex-end;
	justify-content:center;
	margin-right:10rpx;
}
.totall{
	margin-bottom:10rpx;
}
.basefz{
	font-size:28rpx;
}
.red{
	color:red;
}
